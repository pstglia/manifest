<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aosp"
           fetch="https://android.googlesource.com/" />
  <remote  name="x86"
           fetch="https://github.com/pstglia" />
  <default revision="refs/tags/android-5.1.1_r24"
           remote="aosp"
           sync-c="true"
           sync-j="1" />

  <!-- from x86 port repositories -->
  <project path="build" name="build" groups="pdk,tradefed" remote="x86" revision="lollipop-x86_fork" >
    <copyfile src="core/root.mk" dest="Makefile" />
  </project>
  <project path="art" name="platform-art" groups="pdk" remote="x86" revision="lollipop-x86_fork" />
  <project path="bionic" name="bionic" groups="pdk" remote="x86" revision="lollipop-x86_fork" />
  <project path="bootable/newinstaller" name="bootable-newinstaller" remote="x86" revision="lollipop-x86_fork" />

  <!-- from original Android repositories -->
  <project path="external/jpeg" name="platform/external/jpeg" groups="pdk" />
  <project path="tools/tradefederation" name="platform/tools/tradefederation" groups="notdefault,tradefed" />

</manifest>
